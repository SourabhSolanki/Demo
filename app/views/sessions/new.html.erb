
<div class ="login">
<p><%= flash[:error] unless flash[:error].blank? %></p>
<h1 id="head">Log in</h1> 
<%= form_for(:session, url: login_path) do |f| %>
<%= f.label :email %>
<br>
<%= f.email_field :email %>
<br>
<br>
<%= f.label :password %>
<br>
<%= f.password_field :password %>
<br>
<br>
<%= f.submit "Log in" %>
<% end %>
<p id="footer">New user? <%= link_to "Sign up now!", signup_path %></p>
  
</div>
 